    "@types/jsonwebtoken": "^8.5.6",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
